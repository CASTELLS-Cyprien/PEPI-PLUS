{% extends 'base.html.twig' %}

{% block title %}Partner
{% endblock %}

{% block body %}
	<h1>Partner</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ partner.id }}</td>
			</tr>
			<tr>
				<th>CompanyName</th>
				<td>{{ partner.companyName }}</td>
			</tr>
			<tr>
				<th>ContactDetails</th>
				<td>{{ partner.contactDetails }}</td>
			</tr>
			<tr>
				<th>Created_at</th>
				<td>{{ partner.createdAt ? partner.createdAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_partner_index') }}">back to list</a>

	<a href="{{ path('app_partner_edit', {'id': partner.id}) }}">edit</a>

	{{ include('partner/_delete_form.html.twig') }}
{% endblock %}
